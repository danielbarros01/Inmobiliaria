@model Inmobiliaria.Models.Contrato

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>Esta seguro de querer eliminar?</h3>
<div>
    <h4>Contrato</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Desde)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Desde)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Hasta)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Hasta)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Condiciones)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Condiciones)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Monto)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Monto)
        </dd>
        <dt class = "col-sm-2">
            Inmueble
        </dt>
        <dd class = "col-sm-10">
             @Model.Inmueble.ToString()
        </dd>
        <dt class = "col-sm-2">
            Inquilino
        </dt>
        <dd class = "col-sm-10">
            @Model.Inquilino.ToString()
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
